<html>

<head>
  <meta charset="utf-8">
  <title>Konstantin Boilerplate</title>
  <meta name="description" content="">
  <meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=1">
  <% if (APP_CONFIG.env !== 'development') { %>
    <link rel="stylesheet" type="text/css" href="<%=APP_CONFIG.assetOrigin %>/bundle.css">
  <% } %>
</head>

<body>
  <div id="app"></div>
</body>

<% if (APP_CONFIG.env === 'development') { %>
  <script src="<%=APP_CONFIG.protocol %>://<%=APP_CONFIG.host %>:<%=APP_CONFIG.port %>/dist/bundle.js"></script>
<% } else { %>
  <script src="<%=APP_CONFIG.assetOrigin %>/bundle.js"></script>
  <script type="text/javascript">
    console.log('Build time:', '<%=htmlWebpackPlugin.options.buildTime %>')
  </script>
<% } %>

</html>
